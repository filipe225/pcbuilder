
mixin table_list(product)
    tr
        td= product._id 
        td= product.name
        td
            a(href='/admin/' + product.product_type + '/update_' + product.product_type + '/' + product._id) Update
        td
            a(href='/admin/' + product.product_type + '/delete_' + product.product_type + '/' + product._id) Delete  

